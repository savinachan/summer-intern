
  <div class="item well">
    
    <img class="img-rounded" src="http://fakeimg.pl/250x100/" alt="">
    <p><%= product.id %></p>
    <h3><%= product.title %></h3>
    <p><%= product.description %></p>
    <p class="price"><%= product.price %></p>
    <p><%= product.stores.last.name %></p>
    
    <p> 
    <%= link_to "assign", assignment_store_path(product), :target => "_blank", class:"btn btn-success" %>
    <%= link_to "edit", edit_product_path(product), :target => "_blank", class:"btn btn-warning" %>
    <%= link_to "destroy", product_path(product), method: "delete", data: { confirm: "sure?" }, class:"btn btn-danger" %></p>
    <p><a class="btn btn-primary" href="">View details</a></p> 
    
  </div>
