	<%= form_for user do |f| %>

  <%= f.label :name, "Name" %>
  <%= f.text_field :name %> <br />

 	<%= f.label :tel, "Tel" %>
  <%= f.text_field :tel %> <br /> 

  <%= f.label :email, "Email" %>
  <%= f.text_field :email %> <br />


  <%= f.submit class:"btn btn-warning" %>


<%= link_to 'back', users_path, class:"btn btn-info"%>
	
<% end %>

